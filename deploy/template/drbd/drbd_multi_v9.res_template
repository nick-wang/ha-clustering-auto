resource <RESNAME> {
   net {
      protocol	C;
   }
   disk {
      on-io-error	pass_on;
      c-plan-ahead 10;
      c-max-rate 20M;
      c-fill-target 3M;
   }
   volume 0 {
      device	<DEVICE_VO0>;
      disk	<DISK_VO0>;
      meta-disk	<METADISK_VO0>;
   }
   volume 1 {
      device	<DEVICE_VO1>;
      disk	<DISK_VO1>;
      meta-disk	<METADISK_VO1>;
   }
   connection-mesh {
      hosts	<ALL_NODES>;
   }<ON>
}
